{
  "name": "@klevu/ui",
  "version": "0.0.12",
  "description": "Klevu UI components",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/klevu-ui/klevu-ui.esm.js",
  "repository": {
    "type": "git",
    "url": "https://https://github.com/klevultd/frontend-sdk.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "build:watch": "stencil build --watch",
    "build:all": "npm run build && cd ../klevu-ui-react && npm run build && cd ../klevu-ui-vue && npm run build && cd ../klevu-ui",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --e2e",
    "test.watch": "stencil test --e2e --watchAll",
    "generate": "stencil generate",
    "storybook": "export SET NODE_OPTIONS=--openssl-legacy-provider && start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "push-npm": "npm publish --access public"
  },
  "dependencies": {
    "@klevu/core": "file:../klevu-core",
    "@stencil/core": "^2.18.1",
    "axios": "^1.1.3",
    "nouislider": "15.6.1"
  },
  "contributors": [
    {
      "name": "Juha-Pekka Rajaniemi",
      "email": "juha-pekka.rajaniemi@klevu.com"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@ljcl/storybook-addon-cssprops": "^2.3.5",
    "@stencil/react-output-target": "^0.3.1",
    "@stencil/vue-output-target": "^0.6.2",
    "@storybook/addon-actions": "^6.5.13",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/addon-interactions": "^6.5.13",
    "@storybook/addon-links": "^6.5.13",
    "@storybook/addon-notes": "^5.3.21",
    "@storybook/builder-webpack4": "^6.5.13",
    "@storybook/html": "^6.5.13",
    "@storybook/manager-webpack4": "^6.5.13",
    "@storybook/testing-library": "^0.0.13",
    "@types/jest": "^27.0.3",
    "babel-loader": "^8.2.5",
    "chromatic": "^6.10.5",
    "fast-glob": "^3.2.12",
    "jest": "^27.0.3",
    "jest-cli": "^27.4.5",
    "puppeteer": "^19.1.0"
  },
  "license": "MIT"
}
