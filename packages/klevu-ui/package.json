{
  "name": "@klevu/ui",
  "version": "0.10.4",
  "description": "Klevu UI web components. Klevu provides advanced AI-powered search and discovery solutions for online retailers.",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/klevu-ui/klevu-ui.esm.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/klevultd/frontend-sdk.git",
    "directory": "packages/klevu-ui"
  },
  "keywords": [
    "search",
    "ai powered search",
    "site search",
    "ecommerce",
    "product discovery",
    "category navigation",
    "product listing",
    "web components",
    "ui library"
  ],
  "files": [
    "dist/",
    "loader/",
    "hydrate/"
  ],
  "scripts": {
    "build": "stencil build",
    "build:watch": "stencil build --watch",
    "build:all": "npm run build && cd ../klevu-ui-react && npm run build && cd ../klevu-ui-vue && npm run build && cd ../klevu-ui",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --e2e",
    "test:spec": "NODE_OPTIONS=--experimental-vm-modules NODE_NO_WARNINGS=1 stencil test --spec",
    "test.watch": "stencil test --e2e --watchAll",
    "generate": "stencil generate",
    "generate:parts": "node generateParts.js && prettier -w src/utils/parts.ts",
    "generate:icons": "node downloadIcons.js",
    "storybook": "export SET NODE_OPTIONS=--openssl-legacy-provider && storybook dev -p 6006",
    "build-storybook": "export SET NODE_OPTIONS=--openssl-legacy-provider && storybook build",
    "push-npm": "npm publish --access public"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.5.3",
    "@klevu/core": "4.0.1",
    "@stencil/core": "^4.4.0",
    "axios": "^1.5.1",
    "lit-html": "^2.8.0",
    "markdown-it": "^13.0.2",
    "nouislider": "15.7.1",
    "overlayscrollbars": "^2.3.2"
  },
  "contributors": [
    {
      "name": "Juha-Pekka Rajaniemi",
      "email": "juha-pekka.rajaniemi@klevu.com"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.23.0",
    "@floating-ui/react": "^0.26.0",
    "@stencil/react-output-target": "^0.5.3",
    "@stencil/vue-output-target": "^0.8.6",
    "@storybook/addon-actions": "^7.4.6",
    "@storybook/addon-essentials": "^7.4.6",
    "@storybook/addon-links": "^7.4.6",
    "@storybook/addon-mdx-gfm": "^7.4.6",
    "@storybook/addon-styling": "^1.3.7",
    "@storybook/builder-vite": "^7.4.6",
    "@storybook/mdx2-csf": "^1.1.0",
    "@storybook/testing-library": "^0.2.2",
    "@storybook/theming": "^7.4.6",
    "@storybook/web-components": "^7.4.6",
    "@storybook/web-components-vite": "^7.4.6",
    "@types/jest": "^27.0.3",
    "@types/lodash.merge": "^4.6.7",
    "@types/markdown-it": "^13.0.5",
    "@types/mdx": "^2.0.8",
    "@types/node": "^20.8.3",
    "babel-loader": "^9.1.3",
    "fast-glob": "^3.3.1",
    "highlight.js": "^11.9.0",
    "jest": "^27.0.3",
    "jest-cli": "^27.4.5",
    "js-beautify": "^1.14.9",
    "lit": "^2.8.0",
    "lodash.merge": "^4.6.2",
    "prismjs": "^1.29.0",
    "puppeteer": "^21.3.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-simple-code-editor": "^0.13.1",
    "storybook": "^7.4.6",
    "vite": "^4.4.11"
  },
  "license": "MIT"
}
