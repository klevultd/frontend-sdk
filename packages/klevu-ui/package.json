{
  "name": "@klevu/ui",
  "version": "0.3.1",
  "description": "Klevu UI web components. Klevu provides advanced AI-powered search and discovery solutions for online retailers.",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/klevu-ui/klevu-ui.esm.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/klevultd/frontend-sdk.git",
    "directory": "packages/klevu-ui"
  },
  "keywords": [
    "search",
    "ai powered search",
    "site search",
    "ecommerce",
    "product discovery",
    "category navigation",
    "product listing",
    "web components",
    "ui library"
  ],
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "build:watch": "stencil build --watch",
    "build:all": "npm run build && cd ../klevu-ui-react && npm run build && cd ../klevu-ui-vue && npm run build && cd ../klevu-ui",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --e2e",
    "test.watch": "stencil test --e2e --watchAll",
    "generate": "stencil generate",
    "storybook": "export SET NODE_OPTIONS=--openssl-legacy-provider && storybook dev -p 6006",
    "build-storybook": "export SET NODE_OPTIONS=--openssl-legacy-provider && storybook build",
    "push-npm": "npm publish --access public"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.4.4",
    "@klevu/core": "3.0.1",
    "@stencil/core": "^4.0.1",
    "axios": "^1.4.0",
    "lit-html": "^2.7.5",
    "nouislider": "15.7.1",
    "overlayscrollbars": "^2.2.1"
  },
  "contributors": [
    {
      "name": "Juha-Pekka Rajaniemi",
      "email": "juha-pekka.rajaniemi@klevu.com"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.22.8",
    "@babel/preset-env": "^7.22.7",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@floating-ui/react": "^0.24.6",
    "@ljcl/storybook-addon-cssprops": "^3.1.0",
    "@stencil/react-output-target": "^0.5.3",
    "@stencil/vue-output-target": "^0.8.6",
    "@storybook/addon-actions": "^7.0.26",
    "@storybook/addon-essentials": "^7.0.26",
    "@storybook/addon-links": "^7.0.26",
    "@storybook/addon-mdx-gfm": "^7.0.26",
    "@storybook/addon-styling": "^1.3.2",
    "@storybook/builder-vite": "^7.0.26",
    "@storybook/mdx2-csf": "^1.1.0",
    "@storybook/testing-library": "^0.2.0",
    "@storybook/theming": "^7.0.26",
    "@storybook/web-components": "^7.0.26",
    "@storybook/web-components-vite": "^7.0.26",
    "@storybook/web-components-webpack5": "^7.0.26",
    "@types/jest": "^27.0.3",
    "@types/lodash.merge": "^4.6.7",
    "@types/mdx": "^2.0.5",
    "@types/node": "^20.4.1",
    "babel-loader": "^9.1.3",
    "fast-glob": "^3.3.0",
    "jest": "^27.0.3",
    "jest-cli": "^27.4.5",
    "lit": "^2.7.6",
    "lodash.merge": "^4.6.2",
    "puppeteer": "^20.8.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.0.26",
    "vite": "^4.4.2"
  },
  "license": "MIT"
}
