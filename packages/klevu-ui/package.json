{
  "name": "@klevu/ui",
  "version": "0.1.4",
  "description": "Klevu UI web components. Klevu provides advanced AI-powered search and discovery solutions for online retailers.",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/klevu-ui/klevu-ui.esm.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/klevultd/frontend-sdk.git",
    "directory": "packages/klevu-ui"
  },
  "keywords": [
    "search",
    "ai powered search",
    "site search",
    "ecommerce",
    "product discovery",
    "category navigation",
    "product listing",
    "web components",
    "ui library"
  ],
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "build:watch": "stencil build --watch",
    "build:all": "npm run build && cd ../klevu-ui-react && npm run build && cd ../klevu-ui-vue && npm run build && cd ../klevu-ui",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --e2e",
    "test.watch": "stencil test --e2e --watchAll",
    "generate": "stencil generate",
    "storybook": "export SET NODE_OPTIONS=--openssl-legacy-provider && start-storybook -p 6006",
    "build-storybook": "export SET NODE_OPTIONS=--openssl-legacy-provider && build-storybook",
    "push-npm": "npm publish --access public"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.2.3",
    "@klevu/core": "2.0.1",
    "@stencil/core": "^3.1.0",
    "axios": "^1.3.4",
    "nouislider": "15.7.0",
    "overlayscrollbars": "^2.1.0"
  },
  "contributors": [
    {
      "name": "Juha-Pekka Rajaniemi",
      "email": "juha-pekka.rajaniemi@klevu.com"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@ljcl/storybook-addon-cssprops": "^2.3.5",
    "@stencil/react-output-target": "^0.4.0",
    "@stencil/vue-output-target": "^0.7.0",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/addon-notes": "^5.3.21",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/mdx2-csf": "^0.0.4",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/theming": "^6.5.16",
    "@storybook/web-components": "^6.5.16",
    "@types/jest": "^27.0.3",
    "@types/lodash.merge": "^4.6.7",
    "@types/mdx": "^2.0.3",
    "@types/node": "^18.11.19",
    "babel-loader": "^8.2.5",
    "fast-glob": "^3.2.12",
    "jest": "^27.0.3",
    "jest-cli": "^27.4.5",
    "lit-html": "^2.6.1",
    "lodash.merge": "^4.6.2",
    "puppeteer": "^19.7.2"
  },
  "license": "MIT"
}
