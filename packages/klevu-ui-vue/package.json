{
  "name": "@klevu/ui-vue",
  "sideEffects": false,
  "version": "0.10.4",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.esm.js",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    }
  },
  "description": "Klevu UI components for Vue",
  "repository": {
    "type": "git",
    "url": "https://github.com/klevultd/frontend-sdk.git",
    "directory": "packages/klevu-ui-vue"
  },
  "scripts": {
    "test": "echo 'No tests'",
    "build": "npm run clean && npm run bundle",
    "tsc": "tsc -p .",
    "clean": "rm -rf dist && mkdir dist",
    "bundle": "rollup --config rollup.config.js --bundleConfigAsCjs && cp node_modules/@klevu/ui/dist/klevu-ui/klevu-ui.css dist"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@klevu/ui": "0.10.4"
  },
  "devDependencies": {
    "@babel/types": "^7.19.4",
    "@rollup/plugin-typescript": "^9.0.2",
    "rollup": "^3.2.3",
    "tslib": "^2.4.0",
    "typescript": "^4.8.4",
    "vue": "^3.2.41"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ]
}
