{
  "name": "@klevu/core",
  "version": "0.0.8",
  "description": "Typescript SDK that simplifies development on Klevu backend.",
  "keywords": [
    "klevu",
    "search",
    "recommendation",
    "ecommerce tools"
  ],
  "homepage": "https://developers.klevu.com",
  "main": "./dist/index.js",
  "exports": "./dist/index.js",
  "type": "module",
  "scripts": {
    "build:watch": "tsc --watch",
    "build": "rm -rf dist && tsc",
    "qoc": "eslint src/",
    "docs": "typedoc --entryDocument '' --plugin typedoc-plugin-markdown --out docs src/index.ts",
    "test": "NODE_OPTIONS=--experimental-vm-modules NODE_NO_WARNINGS=1 npx jest src --coverage"
  },
  "author": "Klevu",
  "contributors": [
    {
      "name": "Juha-Pekka Rajaniemi",
      "email": "juha-pekka.rajaniemi@klevu.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/klevultd/frontend-sdk",
    "directory": "packages/klevu-core"
  },
  "license": "MIT",
  "typedocMain": "src/index.ts",
  "dependencies": {
    "axios": "^0.26.0",
    "lodash.clonedeep": "^4.5.0",
    "type-fest": "^2.11.2"
  },
  "devDependencies": {
    "@types/lodash.clonedeep": "^4.5.6",
    "typedoc": "^0.22.12",
    "typedoc-plugin-markdown": "^3.11.14"
  }
}
