{
  "name": "@klevu/ui-react",
  "sideEffects": false,
  "version": "0.0.14",
  "description": "Klevu UI components for React",
  "repository": {
    "type": "git",
    "url": "https://https://github.com/klevultd/frontend-sdk.git"
  },
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rm -rf dist && mkdir dist",
    "compile": "cp node_modules/@klevu/ui/dist/klevu-ui/klevu-ui.css dist && rollup --config rollup.config.js",
    "compile:watch": "cp node_modules/@klevu/ui/dist/klevu-ui/klevu-ui.css dist && rollup --config rollup.config.js -w",
    "tsc": "tsc -p .",
    "test": "echo 'No tests'"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.esm.js",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist/"
  ],
  "type": "module",
  "devDependencies": {
    "@types/node": "^18.8.4",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "jest-dom": "^3.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@rollup/plugin-typescript": "^9.0.2",
    "rollup": "^3.2.3",
    "tslib": "^2.4.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@klevu/ui": "0.0.14"
  },
  "jest": {
    "preset": "ts-jest",
    "setupTestFrameworkScriptFile": "<rootDir>/jest.setup.js",
    "testPathIgnorePatterns": [
      "node_modules",
      "dist"
    ]
  }
}
